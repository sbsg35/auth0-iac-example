{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES6",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": ["src/**/*", "m2m_token.js"],
  "exclude": ["node_modules", "dist"]
}
